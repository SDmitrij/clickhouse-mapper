{
  "autoload": {
    "psr-4": {
      "Mapper\\": "src/",
      "Entity\\": "entity/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Mapper\\Test\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "name": "clickhouse-mapper/clickhouse-mapper",
  "require": {
    "php": ">=7.4",
    "doctrine/annotations": "1.13.2",
    "php-di/php-di": "6.3.5",
    "webmozart/assert": "1.10.0",
    "symfony/cache": "4.4.*",
    "smi2/phpclickhouse": "1.4.1",
    "ext-json": "*",
    "symfony/yaml": "v4.4.*",
    "symfony/console": "v4.*"
  },
  "require-dev": {
    "phpunit/phpunit": "9.5.10",
    "overtrue/phplint": "^3.0.6",
    "roave/security-advisories": "dev-latest",
    "squizlabs/php_codesniffer": "^3.6.2"
  }
}
